<div class="content-wrapper">
    <!-- Content Header (Page header) -->

    <!-- Main content -->
    <section class="content">
        <div id="block_content" class="box">
            <div id="content_main" class="box-body">
                <table id="kc_config_table" class="table table-bordered table-striped">
                    <thead>
                    <tr>
                        <th class="">图片<span class="arrow asc"></span></th>
                        <th><label>操作</label></th>
                    </tr>
                    </thead>
                    <tbody>
                        <% for(var i in reglist) {%>
                            <tr>
                                    <td style="height:50px"><div id="preview">
                                        <image src="<%= reglist[i].path %>" style="width:5rem;height:5rem" >
                                    </td>
                                    <td>
                                        <button type="button" value="<%= reglist[i].filename %>" class="btn btn-danger btn-sm" onclick="regdel($(this))">删除</button>
                                    </td>
                                </tr> 
                        <% } %>
                    </tbody>
                    <tfoot>
                    <tr>
                        <td style="height:80px"><div id="preview">
                            <input id='file' class="file" type="file" accept="image/*" name="mc_id"  >
                        </div>
                            <label for="file" class=''>上传</label> 
                        </td>
                        <td>
                            <button type="button" class="btn btn-success btn-sm" onclick="regadd($(this))">增加</button>
                        </td>
                    </tr>
                    </tfoot>
                </table>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->

    </section>
    <!-- /.content -->
</div>